                <option name="programPath" value="/Applications/Beyond Compare.app/Contents/MacOS/BCompare" />
              </ExternalTool>
            </list>
          </value>
        </entry>
        <entry key="MERGE_TOOL">
          <value>
            <list>
              <ExternalTool>
                <option name="argumentPattern" value="%1 %2 %3 %4" />
                <option name="groupName" value="MERGE_TOOL" />
                <option name="name" value="BCompare.real" />
                <option name="programPath" value="/Applications/Beyond Compare.app/Contents/MacOS/BCompare.real" />
              </ExternalTool>
            </list>
          </value>
        </entry>
      </map>
    </option>
    <option name="MERGE_EXE_PATH" value="/Applications/Beyond Compare.app/Contents/MacOS/BCompare.real" />
    <option name="MIGRATE_OLD_SETTINGS" value="true" />
